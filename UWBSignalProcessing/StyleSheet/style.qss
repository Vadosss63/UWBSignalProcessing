	background: qlineargradient(x1:0, y1:1, x2:0, y2:0, stop:0 rgba(1, 1, 5, 80), stop:0.6 rgba(18, 18, 212, 80), stop:0.5 rgba(142, 142, 245, 80) );
	border: 1px solid rgb(18, 18, 212);
}


