TableVRLI QHeaderView::section {
	color: white; 
	background-color: #111111; 
	border: 1px solid white;
}

TableVRLI *{
	color: white;
        background-color: black; border: 1px solid white;
	gridline-color:#ffffff;
}

TableVRLI QTableView:item:selected{
	background-color: qlineargradient(x1:0, y1:1, x2:0, y2:0, stop:0  rgb(255, 0, 50), stop:0.6 rgb(255, 0, 0),stop:1 rgb(255, 0, 50) );

}

TableVRLI QScrollBar:vertical{
	border: 2px solid white;
	background: #111111;
	width: 15px;
	margin: 22px 0 22px 0;
}

TableVRLI QScrollBar::handle:vertical {
	background: #555555;
	min-height: 20px;
}

TableVRLI QScrollBar::add-line:vertical{
	border: 2px solid white;
	background: #444444;
	height: 20px;
	subcontrol-position: bottom;
	subcontrol-origin: margin;
}

TableVRLI QScrollBar::sub-line:vertical{
	border: 2px solid white;
	background: #444444;
	height: 20px;
	subcontrol-position: top;
	subcontrol-origin: margin;
}

TableVRLI QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical{
	border: 2px solid white;
	width: 3px;
	height: 3px;
	background: white;
}

TableVRLI QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}
