}

QGroupBox[name="PRLIGroupBox"] QPushButton { 
	min-width: 16px; 
	min-height: 28px; 
	background: qlineargradient(x1:0, y1:1, x2:0, y2:0, stop:1 rgb(133, 133, 135), stop:0.4 rgb(31, 31, 33,));
